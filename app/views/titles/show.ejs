<div class="page-header">
    <h1>Details of title</h1>
</div>


<table class="table table-bordered">
    <tbody>
        <tr><th>description</th><td><%= title.description %></td></tr>
        <tr><th>status</th><td><%= title.status %></td></tr>
        <tr><th>red_gambler</th><td><%= title.red_gambler %></td></tr>
        <tr><th>red_follower</th><td><%= title.red_follower %></td></tr>
        <tr><th>red_stake</th><td><%= title.red_stake %></td></tr>
        <tr><th>blue_gambler</th><td><%= title.blue_gambler %></td></tr>
        <tr><th>blue_follower</th><td><%= title.blue_follower %></td></tr>
        <tr><th>blue_stake</th><td><%= title.blue_stake %></td></tr>
        <tr><th>owner</th><td><%= title.owner %></td></tr>
        <tr><th>follower</th><td><%= title.follower %></td></tr>    
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', path_to.edit_title(title), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', path_to.title(title), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', path_to.titles()) %>
</div>